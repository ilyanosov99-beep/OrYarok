{
  "name": "driving-school-app",
  "version": "1.0.0",
  "private": true,
  "description": "Web app wrapped with Capacitor",
  "scripts": {
    "start": "npx http-server . -p 8080",
    "cap:init": "npx cap init \"האפליקציה שלי\" com.example.drivingschoolapp --web-dir .",
    "cap:sync": "npx cap sync",
    "cap:android": "npx cap add android",
    "cap:open:android": "npx cap open android"
  },
  "dependencies": {
    "@capacitor/core": "^7.4.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.2",
    "http-server": "^14.1.1"
  }
}
